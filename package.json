    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "sucrase": "^3.34.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-standard": "^12.0.2",
    "typescript": "^5.1.6"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
