{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "Clean node API whit Node.js, Typescropt, TDD, Clean Architecture e SOLID",
  "main": "index.js",
  "scripts": {
    "start": "sucrase-node src/main/server.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:cli": "npm test -- --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheusMendes05/clean-node-api.git"
  },
